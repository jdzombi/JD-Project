          <argument>
            <kind>1</kind>
            <string>//Get Player Input
key_right = keyboard_check(ord("D"));
key_left = -keyboard_check(ord("A"));
key_jump = keyboard_check_pressed(vk_space);
key_shift = keyboard_check(vk_shift)

//React to Input
move = key_left + key_right;
hsp = move * movespeed;
if ((key_shift = 1) &amp;&amp; ((key_right = 1) &amp;&amp; (hsp &lt; MAX_SPEED)) || ((key_left = -1) &amp;&amp; (hsp &gt; - MAX_SPEED))) { 
     movespeed += acc;
     image_speed +=acc;
     }
     
if (move = 0) {
    movespeed = 5  
    image_speed = 1
    }
if ((key_shift = 0) &amp;&amp; (movespeed &gt; 5)) {
     movespeed -= (acc*1.2);
     image_speed -= (acc*1.2);
     }
if (vsp &lt; 10) vsp += grav;

if (place_meeting (x,y+1, obj_GrassTile))
{
    vsp = key_jump * -jumpspeed
}

//Horizontal Collsion
if (place_meeting(x+hsp,y,obj_GrassTile))
{
    while(!place_meeting(x+sign(hsp),y,obj_GrassTile))
    {
        x += sign(hsp);
    }
    hsp = 0;
}
x+= hsp;


//Vertical Collsion
if (place_meeting(x,y+vsp,obj_GrassTile))
{
    while(!place_meeting(x,y+sign(vsp),obj_GrassTile))
    {
        y += sign(vsp);
    }
    vsp = 0;
}

y+= vsp;


//Animate
if (move!=0) image_xscale = move;
if (!place_meeting(x,y+1,obj_GrassTile))
{
    if (vsp &lt; 0) sprite_index = spr_Cowboy_Jump1; else sprite_index = spr_Cowboy_Fall1; // sprite_index = spr_CowboyJump; else sprite_index = spr_CowboyFall;
}
else
{
   image_speed = .33;
   if (hsp==0) 
   {
        sprite_index = spr_Cowboy
   }
   else
   {
       if (hsp!= 0) sprite_index = spr_Cowboy_WalkRight
       else
       {
            sprite_index = spr_Cowboy 
       }
  }
}
</string>
